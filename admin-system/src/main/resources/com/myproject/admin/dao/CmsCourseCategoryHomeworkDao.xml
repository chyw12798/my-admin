<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myproject.admin.dao.CmsCourseCategoryHomeworkDao">

    <insert id="insertList">
        insert into cms_course_cate_homework_relation (course_cate_id,homework_id)
        values
        <foreach collection="list" index="index" separator="," item="item">
            (#{item.courseCateId,jdbcType=BIGINT},
            #{item.homeworkId,jdbcType=BIGINT})
        </foreach>

    </insert>
</mapper>